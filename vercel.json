{
  "buildCommand": "yarn generate",
  "outputDirectory": ".output/public",
  "devCommand": "yarn dev",
  "installCommand": "yarn install",
  "framework": "nuxt",
  "crons": [],
  "functions": {},
  "regions": ["sin1"],
  "cleanUrls": true,
  "trailingSlash": false,
  " headers": [
    {
      "source": "/_nuxt/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
